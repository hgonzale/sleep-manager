{
  "accessory": "HTTP-SWITCH",
  "name": "monteverdi sleep-manager",
  "switchType": "stateful",
  "onUrl": {
      "url": "http://waker_url:51339/waker/wake",
      "headers": {
          "X-API-Key": "YOUR API KEY"
      }
  },
  "offUrl": {
      "url": "http://waker_url:51339/waker/suspend",
      "headers": {
          "X-API-Key": "YOUR API KEY"
      }
  },
  "statusUrl": {
      "url": "http://waker_url:51339/waker/status",
      "headers": {
          "X-API-Key": "YOUR API KEY"
      }
  },
  "statusPattern": {
      "on": "running",
      "off": "stopping|maintenance"
  }
}
